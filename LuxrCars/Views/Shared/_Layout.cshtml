<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="~/Lib/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Lib/materialize/dist/css/materialize.min.css" rel="stylesheet" />


</head>
<body>
    <nav>
        <div class="nav-wrapper">
            <a href="@Url.Action("Index","Home")" class="brand-logo">Luxry CARS</a>
            <ul id="nav-mobile" class="right hide-on-med-and-down">
                <li><a href="@Url.Action("Details","Cart")">Cart</a></li>
                @if(User.Identity.IsAuthenticated)
                {
                <li><a href="@Url.Action("Logout","Account")">LogOut</a><i class="material-icons">arrow_back</i></li>

                }
                @*<li><a href="badges.html">Components</a></li>
                <li><a href="collapsible.html">JavaScript</a></li>*@
            </ul>
        </div>
    </nav>

    <div>
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>
    <script src="~/Lib/jquery/dist/jquery.min.js"></script>
    <script src="~/Lib/materialize/js/jquery-3.2.1.js"></script>
    <script src="~/Lib/materialize/js/slider.js"></script>
    <script src="~/Lib/materialize/js/cards.js"></script>
    <script src="~/Lib/materialize/js/buttons.js"></script>
    <script src="~/Lib/materialize/dist/js/materialize.js"></script>
   
   
</body>
</html>


